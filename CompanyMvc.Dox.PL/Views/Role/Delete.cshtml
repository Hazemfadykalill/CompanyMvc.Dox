
@model RoleViewModel
@{
    ViewData["Title"] = "Delete";
}

<div class=" After shadow px-5 py-3 rounded-2 my-2">
    <div class="w-100 mb-3 text-center">
        <h1 class=" ">  You Can Delete This User @Model.RoleName  !! </h1>
    </div>
    <form asp-action="Delete">

        <partial name="RolePartialView/PartialViewToDetailsAndDelete" model="Model"></partial>


       
        <div class="d-flex justify-content-between  my-3">
            <input type="submit" asp-controller="Role" value="Delete" asp-action="Delete" class="btn btn-success">
            <a asp-action="Index" class="btn btn-warning">Back To List</a>

        </div>
    </form>
</div>
