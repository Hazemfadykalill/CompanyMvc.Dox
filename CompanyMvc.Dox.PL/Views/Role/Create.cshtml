@model RoleViewModel
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="w-100 m-5 text-center bg-dark shadow p-5 rounded-3">
    <h1 class="text-white ">Create New Role </h1>
</div>
<form asp-action="Create">
    <partial name="RolePartialView/PartialViewToCreateAndUpdate"></partial>

    <input type="submit" value="Create" class="btn btn-primary my-2 " />
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>


@section ClientSideValidationByHazem {

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}