
@model EmployeeViewModel
@{
    ViewData["Title"] = "Delete";
}

<div class="DeleteEmployee After shadow px-5 py-3 rounded-2 my-2 ">

    <div class="w-100 mb-3 text-center">
        <h1 class=" ">  You Can Delete @Model.Name !! </h1>
    </div>
    <form asp-action="Delete" enctype="multipart/form-data">

        <partial name="PartialView/PartialViewToDetailsAndDelete" model="Model"></partial>
        <input hidden asp-for="ImageName" />

   

        <div class="d-flex justify-content-between  my-3">
            <input type="submit" asp-controller="Employee" value="Delete" asp-action="Delete" class="btn btn-danger">
            <a asp-controller="Employee" asp-action="Index" class="btn btn-warning">Back To List</a>
        </div>
    </form>
</div>
